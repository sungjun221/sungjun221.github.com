sb.append("SELECT TB_USER_MSG_01.ID     \n");
sb.append(",'01' MSG_BOX                \n");
sb.append(",TB_USER_MSG_01.CUST_ID      \n");
sb.append(",TB_USER_MSG_01.READ_YN      \n");
sb.append(",TB_USER_MSG_01.REG_DATE     \n");
sb.append(",TB_USER_MSG_01.UPT_DATE \n");
sb.append(",TB_MSG.PUSH_TITLE \n");
sb.append(",TB_MSG.MAP1 \n");
sb.append(",TB_MSG.MAP2 \n");
sb.append(",TB_MSG.PUSH_IMG    \n");
sb.append(",TB_MSG.PUSH_VALUE  \n");
sb.append("FROM TB_USER_MSG_01 \n");
sb.append("JOIN TB_MSG \n");
sb.append("WHERE TB_USER_MSG_01.MSG_ID = TB_MSG.ID \n");
sb.append("AND TB_USER_MSG_01.REG_DATE >= DATE_ADD(NOW(), INTERVAL -1 HOUR) \n");
sb.append("AND TB_USER_MSG_01.GRP_ID = 1 \n");
sb.append("UNION ALL \n");
